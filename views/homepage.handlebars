
{{#each projects as |project|}}
  <div class="row">
    <div class="col-md-4">
      <img src="/images/{{project.project_image}}" class="card-img" alt="get moving image" />
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h5 class="card-title"> {{project.title}}</h5>
        <p class="card-text"> {{project.description}}</p>
        <p class="card-text">
          <div class="links">
          <a class="project-link" href="{{project.repo_url}}" >Repo</a>
          
          <a  class="project-link"  href="{{project.deployed_url}}">Deployed</a>
          </div></p>
      </div>
    </div>
  </div>

{{/each}}

<div class="alert alert-secondary" role="alert">
 Skills
</div>

<div class="row"></div>
{{#each skills as |skill|}}
<ul>
{{> skills }}
</ul>





{{/each}}
</div>


